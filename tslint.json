{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-react",
        "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
        "interface-name" : [true, "never-prefix"],
        "no-empty-interface": false,
        "object-literal-sort-keys": false,
        "jsx-no-lambda": false,
        "member-ordering": false,
        "no-trailing-whitespace": true,
        "indent": [
          true,
          "spaces",
          2
        ],
        "linebreak-style": [
          true,
          "LF"
        ]
    },
    "rulesDirectory": []
}
